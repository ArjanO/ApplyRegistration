<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>

    <bean id="logger" class="nl.han.dare2date.logger.LoggerMock" />

    <bean id="jmsSubscriber" class="nl.han.dare2date.jms.JMSSubscriberMock">
    </bean>

    <bean id="userNotification" class="nl.han.dare2date.service.UserNotification">
        <constructor-arg ref="jmsSubscriber"></constructor-arg>
    </bean>
</beans>